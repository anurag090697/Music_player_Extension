{
    "manifest_version": 3,
    "name": "InternetSpeed test",
    "version": "1.0",
    "description": "Monitors your internet connection speed",
    "permissions": ["activeTab"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "images.png"
        }
    },
    "host_permissions": [ "<all_urls>"],
    "icons": {
        "48": "images.png"
    }
}